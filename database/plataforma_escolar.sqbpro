se><tab_sql><sql name="SQL 1*">DELETE FROM usuarios;

-- Triggers para mantener la integridad
CREATE TRIGGER check_docente_tipo
BEFORE INSERT ON docentes
BEGIN
    SELECT 
        CASE 
            WHEN (SELECT tipo FROM usuarios WHERE id = NEW.usuario_id) != 'docente' 
            THEN RAISE(ABORT, 'El usuario debe ser de tipo docente')
        END;
END;

CREATE TRIGGER check_padre_tipo
BEFORE INSERT ON padres
BEGIN
    SELECT 
        CASE 
            WHEN (SELECT tipo FROM usuarios WHERE id = NEW.usuario_id) != 'padre' 
            THEN RAISE(ABORT, 'El usuario debe ser de tipo padre')
        END;
END;

-- Triggers para actualizaciones
CREATE TRIGGER check_docente_tipo_update
BEFORE UPDATE ON docentes
BEGIN
    SELECT 
        CASE 
            WHEN (SELECT tipo FROM usuarios WHERE id = NEW.usuario_id) != 'docente' 
            THEN RAISE(ABORT, 'El usuario debe ser de tipo docente')
        END;
END;

CREATE TRIGGER check_padre_tipo_update
BEFORE UPDATE ON padres
BEGIN
    SELECT 
        CASE 
            WHEN (SELECT tipo FROM usuarios WHERE id = NEW.usuario_id) != 'padre' 
            THEN RAISE(ABORT, 'El usuario debe ser de tipo padre')
        END;
END;

